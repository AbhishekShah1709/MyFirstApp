WindowManager:
    Start:
    PayMoney:
    History:
    AddClothes:
    FixPrice:
    Show:


<TextInput@TextInput>:
    input_type: 'number'
    input_filter: 'int'
    multiline: False

<Start>:
    name: "start"
    canvas:
        Color:
            rgb: [10000, 10000, 10000]
        Rectangle:
            pos: self.pos
            size: self.size

    Label:
        text: "DHOBIGHAT"
        size_hint: 1,0.2
        font_size: 50
        pos_hint: {"x": 0, "y":0.8}
        color: 10000,10000,10000,1
        bold: True
        canvas.before:
            Color:
                rgba: 0.74, 0.2, 0.3, 1
            Rectangle:
                pos: self.pos
                size: self.size


    Button:
        background_color: 10000,10000,10000,1
        text: "Fix Price"
        bold: True
        size_hint: 1,0.12
        pos_hint: {"x": 0,"y":0.6}
        color: 0.9, 0.15, 0.15, 1
        on_release:
            app.root.current = "price"
            root.manager.transition.direction = "left"

    Button:
        background_color: 10000,10000,10000,1
        text: "Add Clothes"
        bold: True
        size_hint: 1,0.12
        color: 0, 0, 0, 1
        pos_hint: {"x": 0,"y":0.48}
        color: 0.9, 0.15, 0.15, 1
        on_release:
            app.root.current = "addclothes"
            root.manager.transition.direction = "left"
    Button:
        background_color: 10000,10000,10000,1
        bold: True
        text: "See history"
        size_hint: 1,0.12
        pos_hint: {"x": 0,"y":0.36}
        color: 0.9, 0.15, 0.15, 1
        on_release:
            app.root.current = "history"
            root.manager.transition.direction = "left"
    Button:
        background_color: 10000,10000,10000,1
        bold: True
        text: "Clear history"
        size_hint: 1,0.12
        pos_hint: {"x": 0,"y":0.24}
        color: 0.9, 0.15, 0.15, 1
        on_release:
            root.clear_history()
            app.root.current = "history"
            root.manager.transition.direction = "left"
    Button:
        background_color: 10000,10000,10000,1
        bold: True
        text: "Pay Money"
        size_hint: 1,0.12
        pos_hint: {"x": 0,"y":0.12}
        color: 0.9, 0.15, 0.15, 1
        on_release:
            app.root.current = "pay"
            root.manager.transition.direction = "left"

    Button:
        background_color: 10000,10000,10000,1
        bold: True
        text: "Show Amount Pending"
        size_hint: 1,0.12
        pos_hint: {"x": 0,"y":0.0}
        color: 0.9, 0.15, 0.15, 1
        on_release:
            app.root.current = "show"
            root.manager.transition.direction = "left"



<Show>:
    name: "show"
    amountt: amountt

    FloatLayout:
        cols: 1
        Label:
            id: amountt
            text: ""
            font_size: 60
            color: 0.74, 0.2, 0.3, 1
            pos_hint: {"x": 0, "y": 0.1}
            size_hint: 1, 1
            bold: True
            canvas.before:
                Color:
                    rgba: 10000,10000,10000,1
                Rectangle:
                    pos: self.pos
                    size: self.size

        Button:
            text: "Back"
            pos_hint: {"x": 0, "bottom": 1}
            size_hint: 1, 0.1
            background_color: 2, 0.2, 0.5, 1
            color: 10000,10000,10000,1
            on_press:
                app.root.current="start"
                root.manager.transition.direction = "right"


<PayMoney>:
    name: "pay"
    money: money
    canvas:
        Color:
            rgb: [10000, 10000, 10000]
        Rectangle:
            pos: self.pos
            size: self.size

    FloatLayout:
        cols: 1
        FloatLayout:
            cols: 1
            Label:
                text: "Please input the amount paid"
                font_size: 60
                pos_hint: {"x": 0, "top":0.9}
                size_hint: 1,0.1
                color: 0.74, 0.2, 0.3, 1
                bold: True
                canvas.before:
                    Color:
                        rgba: 10000,10000,10000,1
                    Rectangle:
                        pos: self.pos
                        size: self.size
            TextInput:
                input_type: 'number'
                input_filter: 'int'
                pos_hint: {"x": 0, "y":0.65}
                size_hint: 1,0.1
                multiline: False
                bold: True
                foreground_color: 0.74, 0.2, 0.3, 1
                id: money
                padding: [self.width/3,(self.height-self.line_height)/2]

        Button:
            text: "Done"
            pos_hint: {"x": 0, "bottom":1}
            size_hint: 1,0.1
            background_color: 2, 0.2, 0.5, 1
            color: 10000,10000,10000,1
            on_press:
                root.submit()
                app.root.current="start"
                root.manager.transition.direction = "right"

