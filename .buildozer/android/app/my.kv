                pos_hint: {"x": 0.33, "top": 1}
                size_hint: 0.33, 0.1
                canvas.before:
                    Color:
                        rgba: 10000,10000,10000,1
                    Rectangle:
                        pos: self.pos
                        size: self.size
            Label:
